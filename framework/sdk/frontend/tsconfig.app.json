{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "commons/**/*", "commons/**/*.vue", "src/**/*", "src/**/*.vue", "packages/**/*"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@commons/*": ["./commons/*"],
    }
  }
}
